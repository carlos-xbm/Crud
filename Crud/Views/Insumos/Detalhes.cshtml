@model InsumoModel

<div>
    <h1>Registro de insumo</h1>

    <input type="hidden" asp-for="Id" />


    <div class="mt-3">
        <label class="form-label">Nome:</label>
        <input type="text" class="form-control" asp-for="Nome" disabled />
    </div>
    <div class="mt-3">
        <label class="form-label">Fornecedor:</label>
        <input type="text" class="form-control" asp-for="Fornecedor" disabled />
    </div>
    <div class="mt-3">
        <label class="form-label">NF:</label>
        <input type="text" class="form-control" asp-for="NF" disabled />
    </div>
    <div class="mt-3">
        <label class="form-label">Lote:</label>
        <input type="text" class="form-control" asp-for="Lote" disabled />
    </div>
    <div class="mt-3">
        <label class="form-label">Quantidade:</label>
        <input type="number" class="form-control" asp-for="Quantidade" disabled />
    </div>
    <div class="mt-3">
        <label class="form-label">Fabricação:</label>
        <input type="date" class="form-control" asp-for="Fabricacao" disabled />
    </div>
    <div class="mt-3">
        <label class="form-label">Vencimento:</label>
        <input type="date" class="form-control" asp-for="Vencimento" disabled />
    </div>
    <div class="mt-3">
        <label class="form-label">Entrada:</label>
        <input type="date" class="form-control" asp-for="Entrada" disabled />
    </div>
</div>